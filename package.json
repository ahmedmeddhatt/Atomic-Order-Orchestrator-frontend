{
  "name": "fincart-monorepo",
  "private": true,
  "workspaces": [
    "fincart-backend",
    "fincart-frontend"
  ],
  "scripts": {
    "dev:backend": "npm run start:dev -w fincart-backend",
    "dev:frontend": "npm run dev -w fincart-frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "npm run build -w fincart-backend && npm run build -w fincart-frontend",
    "install:all": "npm install"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
